public class Activity1{

    public static void main(String[] args) {

        int num = 10;
        char let = 'a';
        boolean tru = true;
        String lett = "Hello";

        System.out.println("The Value of integer is: " + num);
        System.out.println("The Value of Character Data Type is: " + let);
        System.out.println("The Value of Boolean is: " + tru);
        System.out.println("The Value of String is: " + lett);
    }
}
